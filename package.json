{
  "name": "bolao-raro-academy",
  "version": "1.2.2",
  "description": "API para bolao do Campeonato Brasileiro",
  "main": "build/index.js",
  "scripts": {
    "compile": "npx tsc",
    "dev": "npx ts-node app.ts",
    "lint": "npx eslint -c .eslintrc.js src/**/*.ts",
    "lint-fix": "npx eslint -c .eslintrc.js src/**/*.ts --fix",
    "start": "ts-node src/app.ts",
    "test": "npx jest --coverage",
    "orm": "ts-node node_modules/typeorm/cli.js",
    "orm:generate": "ts-node node_modules/typeorm/cli.js migration:generate -n"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/{!(mocksCadastro),}.ts"
    ]
  },
  "author": "viniderp@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^8.0.29",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "3.3.0",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "axios": "^0.23.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.14.1",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.38"
  }
